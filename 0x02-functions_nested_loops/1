#include "main.h"
#include <string.h>

/**
  * print_to_98 - prints all natural numbers from n to 98
  * @n: number to start
  * Return: nothing
  */

void print_to_98(int n)
{
	int a = n;

	if (a == 98)
	{
		_putchar('9');
		_putchar('8');
		_putchar('\n');
	}
	else if (a < 98)
	{
		while (a <= 98)
		{
			if (a != n)
				_putchar(' ');

			if ((a > -10) && (a < 10))
			{
				_putchar(a + '0');
			}
			else
			{
				_putchar((a / 10) + '0');
				_putchar((a % 10) + '0');
			}

			if (a != 98)
				_putchar(',');
			a++;
		}
	}
	else
	{
		while (a >= 98)
		{
			if (a != n)
				_putchar(' ');

			char b[];

			sprintf(b, "%d", a);
			int c = strlen(b);

			for (int i = 0; i < c; i++)
			{
				_putchar(b[i]);
			}
			if (a != 98)
				_putchar(',');
			a++;
		}
	}
	_putchar('\n');
}
